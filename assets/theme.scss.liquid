#shopify-section-header-tracking{  


  // Medium screens
  @include at-query ($max, $medium) {
    top: 102px; /* height of header */  
    
    #nav-bar-page {  
      .nav-bar-page-head{
        label, select{
          font-size: 17px;
        }
      }
    }
  }

  position: relative;
  left: 0;
  top: 89px; /* height of header */  
  z-index: 200; /* make sure under/less the main nav-bar submenu/dropdown */
  width: 100%;
  color: white;
  background-color: #01c19e;

  #nav-bar-page {   
    width: 100%;
    text-align: center;    
      
    .nav-bar-page-head{
      padding-top: 15px;
      padding-bottom: 15px;

      .form{
        margin: 0;

        label, select{
          display: inline-block;
          margin: 0;
          font-size: 19px;
          font-weight: 600;
        }

        label{
          margin-right: 10px;
        }

        select{
          margin: 0;
          width: auto;
          min-width: 160px;
          padding-top: 3px;
          padding-bottom: 4px;

          color: white;
          border-color: white;
          border-width: 2px;
          background-color: darken(#01c19e, 5%);
          background-image: url('{{ 'icon-arrow-down.svg' | asset_url }}');
        }

        // Small screens
        @include at-query ($max, $small) {
          label, select{
            font-size: 17px;
          }

          labe{
            margin-right: 0;
          }

          select {
            min-width: 130px;
            padding-top: 2px;
            padding-bottom: 2px;
          }
        }
      }
    }

    .nav-bar-page-body{
      display: none; /* hide by default */
      background-color: #5d5d5d;
      {% comment %} background-color: mix(#01c19e, #3e3e3e, 10%); {% endcomment %}

      .nav-bar-page-content{      
        
        padding-top: 30px;
        padding-bottom: 0;
        text-align: left;
        color: #f7f7f7;

        a{
          color: #05be9c;
          text-decoration: none;

          &:hover{
            text-decoration: underline;
          }
        }

        .track-head{
          
          padding-bottom: 40px;
          font-size: 19px;
          font-weight: 500;
          text-align: left;

          .track-title{
            margin-bottom: 40px;

            img{
              display: inline-block;
              height: 70px;
              vertical-align: middle;
            }

            h4{
              display: inline-block;
              margin: 0;
              padding-top: 10px;
              font-size: 34px;
              font-weight: 600;
              line-height: 1.4;
              vertical-align: middle;
            }
          }

          // Medium Down screens
          @include at-query ($max, $medium) {

            .track-title{
              text-align: center;

              img{
                display: block;
                margin: 0 auto;
              }

              h4{
                display: block;
                margin: 0 auto;
              }
            }
          }

          .track-title + .lead{
            font-size: 24px;
            line-height: 1.4;
            font-weight: 500;
            text-align: left;
            margin: 0;
            margin-bottom: 30px;
            padding: 0;
            color: white;
          }
          
          ol{
            margin-bottom: 20px;

            > li{
              font-weight: 500;
              margin-bottom: 8px;
              margin-left: 16px;
            }
          }
         
          p{
            margin-bottom: 20px;

            &:last-child{
              margin-bottom: 0;
            }

            img{
              vertical-align: bottom;
            }
          }

          .icons-wrapper{
            display: inline-block;
            white-space: nowrap;
          }
        }

        .track-foot{
        
          padding: 30px 0 30px 0;
          font-size: 19px;
          font-weight: 500;
          font-style: italic;
          background-color: #6f6f6f;
          {% comment %} background-color: mix(#01c19e, #3e3e3e, 20%); {% endcomment %}

          p{
            margin-bottom: 4px;

            &.light{
              color: #ccc;
              {% comment %} color: mix(#01c19e, #fff, 10%); {% endcomment %}
            }
          }
        }
      }

      .nav-bar-page-toolbar{
        display: none;
        padding: 5px;
        text-align: center;
        background-color: darken(#01c19e, 5%);

        a{
          display: block;
          padding: 5px;
          line-height: 20px;
          width: 100%;

          img{
            margin: 0 auto;
            vertical-align: bottom;
          }
        }
      }
    }
  }   






}

