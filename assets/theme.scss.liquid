/*================ Small screen nav styles ================*/
@include at-query ($max, $small) {
  .nav-bar {
    .show-nav & {
      bottom: 0;
      padding: 0;
      top: 52px;
      width: 100%;
    }
  }

  .show-nav,
  .show-nav body {
    height: 100%;
    overflow: hidden;
  }
}

/*================ Medium-down screen nav styles ================*/
@include at-query ($max, $medium) {
  .nav-bar {
    position: fixed;

    z-index: 2000;
    @include transition(all 600ms cubic-bezier(0.47, 0.5, 0, 0.95));

    .show-nav & {
      max-height: 100%;
      overflow-y: scroll;
      -webkit-overflow-scrolling: touch;
      @include transition(all 450ms cubic-bezier(0.47, 0, 0, 0.95));
    }

    .wrapper {
      padding: $gutter/2 0 0;

      .show-nav & {
        min-height: 101%;
      }
    }

    .search-bar {
      margin: 0 $gutter/2 $gutter/2;
      width: auto;
      input[type="search"]::-webkit-input-placeholder {
        color: color-control($colorNav);
      }
      input::-webkit-input-placeholder {
        color: color-control($colorNav);
      }
      .search-bar--submit .icon {
        color: color-control($colorNav);
      }
    }
  }

  .site-nav {
    border-top: 1px solid lighten($colorNav, 10%);
  }

  /*================ Menu items ================*/
  .site-nav > li {
    border-bottom: 1px solid lighten($colorNav, 20%);
    -webkit-tap-highlight-color: rgba(255,255,255,0.2);

    &:hover {
      background-color: lighten($colorNav, 10%);
    }
  }
}

/*================ Medium screen size only ================*/
@include at-query (null, $postSmall, $medium) {
  .nav-bar {
    .show-nav & {
      top: $mobileNavBarHeight;
      bottom: 0; padding:0; width:100%;
    }
  }




}

/*================ Large screen bar navigation ================*/
@include at-query ($min, $large) {
  .nav-bar {
    .search-bar {
      display: none;
    }
  }

  .site-nav {
    text-align: left;
    white-space: nowrap;
    margin-left: -$gutter/2;

    & > li {
      position: static;
      display: inline-block;
    }

    a {
      white-space: nowrap;
    }

  }
}

/*================ Dropdowns ================*/
.icon-arrow-down {
  position: relative;
  top: 1px;
  font-size: 0; background:url(arrow-down.png) no-repeat center center; width:20px; height:10px; vertical-align: middle;
}



.site-nav--dropdown {
  display: none;
  margin: 0;width:100%;
}

.site-nav--dropdown .icon-arrow-down {
  display: none !important;
}





/*============================================================================
# Standard Navigation
==============================================================================*/

@include at-query ($min, $large) {

  .site-nav--dropdown { 
    overflow: hidden;
    visibility: hidden;  
    opacity: 0;

    position: absolute;
    top: 100%;
    left: 0;

    display: block; 
    width:100%;
    min-height: 40px;
    min-width: 150px;
    padding: 40px 0;
    border: 0 solid $colorBorder;
    background-color: #fdfdfd;
    box-shadow: 0 1px 1px rgba(0,0,0,0.25);
    z-index: 20;    
    @include transition(all 150ms);

    .no-touch .site-nav--has-dropdown:hover &,
    .site-nav--has-dropdown.nav-hover &,
    .nav-focus + &,
    .show-dropdown & {
      opacity: 1;
      visibility: visible;
      @include transition(all 90ms);
    }
  }
}

.site-nav--dropdown  {

  &:before{ box-shadow:0 0 4px rgba(1, 1, 1, 0.85); content:""; height:4px; width:100%; position:absolute; top:-4px; left:0; }

  a {
    display: block;
    padding: 0;
    font-size: 15px;
    color: lighten($colorNavText, 5%);
    text-transform: none;

    &:after{
      display: inline-block;
      margin-left: 5px;
      font-family: FontAwesome;
      content: '\f144';
    }

    &:hover,
    &:active,
    &:focus {
      color:#00c1a1; 
      text-decoration:underline;

      span{
        opacity:1
      }
    }
  }

  .site-nav--dropdown-list
  {
    display: flex;
    display: -ms-flex;
    display: -webkit-flex;

    > li.grid__item{ 

      display: inline-block;
      vertical-align: top; 
      white-space: normal;

      &:first-child{
        padding-left: 0;
        padding-right: 40px;
      }

      &:last-child{
        padding-left: 40px;
        padding-right: 20px;
        background: url(//cdn.shopify.com/s/files/1/1801/4767/files/site-nav-vertical-separator.png?1832860389030007524) no-repeat -15px center; 
      }

      .site-nav--dropdown-content{
        clear: both;
        margin-left: 50%;
      }

      h3{
        margin: 0 0 5px 0;
        color: #434343; 
        font-size: 19px; 
        font-weight: 600;
      }

      .lead{
        margin-bottom: 20px;
        padding: 0;
        font-size: 14px;
        text-align: left;
        line-height: 1.1;
      }

      hr{
        max-width: 120px;
        height: 1px;
        margin: 20px 0 20px 0;
        border: none;
        border-bottom: 1px dashed #e4e4e4;
        box-shadow: none;
      }

      p{ 
        margin:0;
        margin-bottom: 10px;
        font-size: 14px;
        line-height: 1.42;
        font-weight: 300; 

        &.price-desc{
          position: relative;
          font-size: 12px;
          margin-top: 2px;
          margin-bottom: 20px;

          &:after{
            position: absolute;
            bottom: 8px;
            font-family: FontAwesome;
            font-size: 5px;
            content: '\f069';
            margin-left: 1px;
          }
        }
      }

      a {
        font-weight: 500;
        color: #00c1a1;
        text-decoration: underline;
        text-transform: none;

        &:hover,
        &:active,
        &:focus {
          color: #00c1a1; 
          text-decoration: none;
        }
      }

      .red{
        a{
          color: #c90909;
        }
      }

      .site-nav--dropdown-inner{

        .site-nav--dropdown-inner-list{
          margin-left: 0;

          > li{
            display: block;
            margin: 10px 0;

            a{
              font-size: 17px;

              &.title{
                display: block;
                margin-bottom: 5px;
                padding-left: 0;
              }
            }
          }
        }
      }


      /*================ Bluetooth ================*/
      &.bluetooth-site-nav-item{

        .site-nav--dropdown-inner{

          .site-nav--dropdown-inner-list{
            > li{

              > div{
                clear: both;
                float: left;
                width: 50%;
                padding-right: 20px;
                padding-bottom: 20px;
              }

              img{
                max-width: 260px;
              }

              &:first-child{
                img{
                  margin-top: -70px;
                }
              }

              &:last-child{
                img{
                  margin-top: -30px;
                }
              }
            }
          }
        }
      }  


      /*================ GPS ================*/
      &.gps-site-nav-item{

        .site-nav--dropdown-inner{
          
          > div{
            float: left;
            width: 50%;
            height: 100%;
            padding-right: 20px;
          }

          img{
            width: 100%;
            max-width: 183px;
            max-height: 234px;
            margin-top: -20px;
            margin-left: -10px;
          }

          .site-nav--dropdown-inner-list{
            float: left; 
            width: 50%;
          }
        }

      }    
    }
  }
}


