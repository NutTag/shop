  max-width: 100%;

  &.input-full {
    width: 100%;
  }
}

input,
select,
textarea {
  font-size: 1em;
  padding: 8px 10px;
  line-height: 1.42;
  background: transparent;
}

.cart-row input[type="submit"],
.cart-row button[type="submit"]  {
